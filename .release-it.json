{
  "git": {
    "tagName": "v${version}",
    "commitMessage": "chore(release): v${version}",
    "requireCleanWorkingDir": true,
    "requireUpstream": false,
    "requireCommits": true,
    "requireBranch": "main",
    "push": true,
    "pushArgs": ["--follow-tags"]
  },
  "github": {
    "release": true
  }
}
